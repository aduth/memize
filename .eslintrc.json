{
	"root": true,
	"extends": "@aduth/eslint-config",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"node": true
	},
	"rules": {
		"semi": "error",
		"no-var": "off",
		"one-var": "off",
		"prefer-spread": "off",
		"jsdoc/check-tag-names": "off",
		"valid-jsdoc": "off",
		"no-redeclare": "off"
	}
}
