{
	"root": true,
	"extends": "@aduth/eslint-config",
	"parserOptions": {
		"ecmaVersion": 5,
		"sourceType": "script"
	},
	"env": {
		"node": true
	},
	"rules": {
		"no-var": "off",
		"one-var": "off",
		"prefer-spread": "off",
		"jsdoc/check-tag-names": "off",
		"valid-jsdoc": "off",
		"no-redeclare": "off",
		"@typescript-eslint/no-redeclare": "error"
	}
}
